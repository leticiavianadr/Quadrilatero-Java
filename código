public class Quadrilatero {

    public static void area(double lado) {

        System.out.println("Área do quadrado: " + lado * lado);
    }
    public static void area(double lado1, double lado2) {

        System.out.println("Área do retângulo: " + lado1 * lado2);
    }
    public static void area(double base1, double base2, double altura) {

        System.out.println("Área do trapézio: " + ((base1 + base2) * altura) / 2);
    }

}
public class Main {

    public static void main(String[] args){
        System.out.println("Exercicío da calculadora");
        Calculadora.soma(6, 3);
        Calculadora.subtracao(9, 1.6);
        Mensagem.obterMensagem(6);
        Quadrilatero.area(9,6);
        Quadrilatero.area(5);


    }
}
